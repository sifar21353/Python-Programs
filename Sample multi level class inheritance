class Animal():
  planet = 'Earth'
  def __init__(self):
    self.mass = ''
    self.height = ''
    self.diet = ''
    self.n_legs = 0
    self.habitat = ''
    self.move = ''
    self.communicate = ''
  
  def breathe():
    return 'I can breathe.'
  
  def reproduce():
    return 'I can reproduce.'

  def think():
    return 'I can think.'

  def eat():
    return 'I can eat.'

  def hunt():
    return 'I can hunt.'

  def live():
    return 'I live.'
  
  def respond_to_stimuli():
    return 'I can respond to stimuli.'

class Cat(Animal):
  def __init__(self):
    self.mass = '4 kg'
    self.height = '24 cm'
    self.diet = 'Omnivorous'
    self.n_legs = 4
    self.habitat = 'Domestic'
    self.move = 'Walk/Run'
    self.communicate = 'Meow'

  def save_myself():
    return 'I can fall on my feet.'

class Tiger(Cat):
  def __init__(self):
    super().__init__()
    self.mass = '188 kg'
    self.height = '95 cm'
    self.habitat = 'Jungles/Forests'
    self.communicate = 'Roar'

  def jump():
    return 'I can jump 5 m.'
